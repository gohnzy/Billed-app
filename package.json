{
  "scripts": {
    "test": "jest",
    "watch": "jest --watch",
    "coverage": "jest --coverage",
    "live": "live-server",
    "lint": "eslint"
  },
  "jest": {
    "verbose": false,
    "setupFiles": [
      "./setup-jest.js"
    ],
    "coveragePathIgnorePatterns": [
      "./jsdom.config.js"
