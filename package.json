  "type": "module",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@types/jquery": "3.5.29",
    "bootstrap": "^5.3.3",
    "eslint": "^9.1.1",
    "express": "4.17.1",
    "fs": "^0.0.1-security",
    "global": "4.4.0",
    "history": "^5.3.0",
    "jest-fetch-mock": "^3.0.3",
    "jquery": "3.7.1",
    "jquery-ui": "1.13.2",
    "live-server": "^1.2.2",
    "path": "0.12.7",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@babel/core": "7.24.4",
    "@babel/preset-env": "7.24.4",
    "@testing-library/dom": "7.20.0",
    "@testing-library/jest-dom": "5.11.0",
    "@testing-library/user-event": "12.0.11",
    "babel-jest": "26.6.3",
    "jest": "26.1.0",
    "jest-environment-jsdom": "27.4.6",
    "jest-html-reporter": "3.1.3"
  }
